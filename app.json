{
  "expo": {
    "name": "Framez",
    "slug": "framez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon.png",
    "scheme": "framez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ifecodes.framez"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icon/light-logo-1024x1024.png",
        "backgroundImage": "./assets/icon/logo-1024x1024.png",
        "monochromeImage": "./assets/icon/logo-1024x1024.png"
      },
      "package": "com.ifecodes.framez",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon/icon1024x1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo-removebg-preview.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#050E1A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "27aca14f-5b80-4565-b806-4aece4906ef3"
      }
    },
    "owner": "ifecodes01",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4ad2bc63-e5a8-45d0-92b0-8ca7ecacad81"
    }
  }
}
